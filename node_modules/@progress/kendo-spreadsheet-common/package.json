{
  "name": "@progress/kendo-spreadsheet-common",
  "description": "Kendo UI platform-independent Spreadsheet library",
  "version": "1.2.5",
  "keywords": [
    "Kendo UI"
  ],
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/index.js",
  "module": "dist/index-esm.js",
  "types": "src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup --config --watch",
    "lint": "eslint src",
    "test": "jest",
    "e2e": "jest --config ./jest.e2e.config.json",
    "e2e:visual": "jest --config ./jest.e2e.visual.config.json",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@progress/jszip-esm": "^1.0.4",
    "@progress/kendo-common": "^1.0.1",
    "@progress/kendo-date-math": "^1.5.10",
    "@progress/kendo-drawing": "^1.17.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@progress/kendo-e2e": "^4.0.0",
    "@progress/kendo-intl": "^3.1.1",
    "@telerik/dx-metrics": "^2.1.8",
    "@types/jest": "^29.0.0",
    "acorn": "^8.12.0",
    "es-jest": "^2.1.0",
    "escodegen": "^2.1.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "jest-cli": "^29.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "rollup": "^3.5.1",
    "rollup-plugin-watch": "^1.0.4",
    "semantic-release": "^24.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-spreadsheet-common.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "release": {
    "branches": [
      {
        "name": "master",
        "channel": "latest"
      },
      {
        "name": "develop",
        "prerelease": true,
        "channel": "dev"
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src/**/*.d.ts"
  ]
}
